<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mds.valentinandre.ziksearch">
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/iconzs"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconzs"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="fr.mds.ziksearch.activity.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="fr.mds.ziksearch.activity.SearchActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>

        <activity android:name="fr.mds.ziksearch.activity.ArtistActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        </activity>
        <activity android:name="fr.mds.ziksearch.activity.AlbumActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        </activity>

        <activity android:name="fr.mds.ziksearch.activity.PreviewActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar">

        </activity>

    </application>

</manifest>